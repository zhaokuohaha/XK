@model IEnumerable<XK.Models.SelectCourseItem>

<div class="transparent-panel" id="deleteShow">
	<table class="table table-border">
		<thead>
			<tr>
				<th>课号</th>
				<th>课名</th>
				<th>任课教师号</th>
				<th>分数</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var item in Model)
			{
				<tr>
					<td>@item.cid </td>
					<td>@item.cname </td>
					<td>@item.ctec </td>
					<td>@item.cscore</td>
				</tr>

			}
		</tbody>
	</table>
</div>
