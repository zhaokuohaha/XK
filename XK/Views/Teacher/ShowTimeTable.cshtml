@model IEnumerable<XK.Models.SelectCourseItem>
@{
    ViewBag.Title = "ShowTimeTable";
    Layout = "~/Views/Home/tecLogin.cshtml";
}

<div class="transparent-panel">
	<table class="table table-hover">
		<thead>
			<tr>
				<th>开课ID</th>
				<th>课程号</th>
				<th>课程名</th>
				<th>上课地点</th>
				<th>上课时间</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var time in Model)
			{
				<tr>
					<th>@time.ccid</th>
					<td>@time.cid</td>
					<td>@time.cname</td>
					<td>@time.caddr</td>
					<td>@time.ctime</td>
				</tr>
			}
		</tbody>
	</table>
</div>


