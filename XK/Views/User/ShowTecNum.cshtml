@model IEnumerable<XK.Models.xk_Teacher>
<div class="panel panel-info">
    <div class="panel-heading">
        <h3>共有 @ViewBag.count 名老师</h3>
    </div>
    <table class="table">
        <tr><th>姓名</th><th>职位</th></tr>
        @foreach (var item in Model)
        {
            <tr>
                <td width="150px;"> @item.tch_name</td>&emsp;&emsp;
                <td> @item.tch_pos</td>
            </tr>
        }
    </table>
</div>

