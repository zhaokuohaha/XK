@model IEnumerable<XK.Models.User>

@{
    ViewBag.Title = "Login";
}

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>测试</title>
</head>
<body>
    <h1>登录成功</h1>
    <hr />
    @ViewBag.user
    @ViewBag.pswd
    <hr />
    <table>
        @foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.u_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.u_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.u_password)
        </td>
    </tr>
}
    </table>
</body>
</html>
